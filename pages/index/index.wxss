/**index.wxss**/
.navs{
  width:100%;
  height: 86rpx;
  position: fixed;
  left:0;
  bottom: 0;
  display: flex;
  justify-content: space-between;
}
.navs>view{
  width:100%;
  height: 86rpx;
  line-height: 86rpx;
  text-align: center;
  color: #fff;
}
.navs>view:nth-child(1){
  background: #ff9700
}
.navs>view:nth-child(2){
  background: #03bbd5
}
.container{
  position: absolute;
  left:0;
  top:0;
  bottom:86rpx;
  right: 0;
}

.container map{
  width: 100%;
  height: 100%;
}